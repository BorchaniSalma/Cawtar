<script>

        var email_is_true = false;

        $(document).ready(function () {
            $('#id_email').val('');

            $(document).on({
                'paste': function (e) {
                    e.preventDefault();
                }
            }, "#id_email_confirm");


            // form handler to registeration form ..
            $('#submit-id-sign-in').click(function (e) {
                e.stopPropagation();
                e.preventDefault();


                if (email_is_true) {
                    var query_string = '';
                    var email = $('#id_email').val();
                    var is_saudi = 'true';
                    if ($('#id_is_saudi_2').is(':checked')) {
                        is_saudi = 'false';
                    }

                    query_string += 'email=' + email + '&is_saudi=' + is_saudi;

                    window.location.href = $(this).attr('href') + '?' + query_string;
                }

            });


        });


        $("#register-form-home .form-control").blur(function () {
            if ($(this).is("select")) {
                ga('send', 'event', 'Form Fields', 'Signup', $(this).find("option:first").text());

            } else {
                ga('send', 'event', 'Form Fields', 'Signup', $(this).attr("placeholder"));

            }

        });


        $('#register-form-home input[data-required="required"]').focusout(function () {
            if (!$(this).val()) {
                $(this).parent('div').addClass('has-error');
                $(this).parent('div').find('span').remove();
                $(this).after('<span id="error_required" class="help-block"><strong class="color-light-red">هذا الحقل مطلوب</strong></span>');
            }
            else {
                $('#error_required').remove();
                $(this).parent('div').removeClass('has-error');
            }
        })


        $('#id_email').blur(function () {
            var my_data = {};
            my_data['email'] = $(this).val();
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            if (isEmptyOrSpaces($(this).val())) {
                email_is_true = false;
                return;
            }
            if (!validateEmail($(this).val())) {
                if ($('#error_email2').length == 0) {
                    $(this).parent('div').addClass('has-error');
                    $('#id_email').after('<span id="error_email2" class="help-block"><strong class="color-light-red"> يوجد خطأ في إدخال بريدك الإلكتروني.</strong></span>');
                    email_is_true = false;
                }
                else {
                    $('#error_email2').remove();
                    $(this).parent('div').removeClass('has-error');
                    email_is_true = true;
                }
            }
            else {
                $('#error_email2').remove();
                $(this).parent('div').removeClass('has-error');
                email_is_true = true;
            }
        });


        function validateEmail(email) {
            var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
            return re.test(email);
        }

        function isEmptyOrSpaces(str) {
            return str === null || str.match(/^ *$/) !== null;
        }

    </script>
	
	<script>
            $("#id_username").change(function () {
                var username = $(this).val();

                $.ajax({
